{
  "name": "ra-react",
  "version": "7.0.0",
  "description": "Starter kit for creating apps with React and Redux",
  "license": "MIT",
  "author": {
    "name": "Remote Assembly LLC",
    "email": "contact@remoteassembly.com",
    "url": "https://www.remoteassembly.com/"
  },
  "homepage": "https://www.remoteassembly.com/",
  "bugs": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "setup": "node tools/setup/setupMessage.js && yarn && node tools/setup/setup.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "standard --verbose --fix 'src/**/*.js' | snazzy && prettier-standard 'src/**/*.js'",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "coverage": "npm run test -- --coverage",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "dependencies": {
    "dotenv-webpack": "1.5.5",
    "lodash": "4.17.4",
    "material-ui": "^1.0.0-beta.18",
    "material-ui-icons": "^1.0.0-beta.17",
    "object-assign": "4.1.0",
    "react": "16.0.0",
    "react-avatar-editor": "10.3.0",
    "react-dom": "16.0.0",
    "react-ga": "2.4.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.6.0",
    "redux-auth-wrapper": "2.0.2",
    "redux-socket.io": "1.4.0",
    "redux-thunk": "2.1.0",
    "snazzy": "^7.0.0",
    "stackdriver-errors-js": "0.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.0.0-beta.40",
    "autoprefixer": "7.1.4",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.6",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.18.12",
    "chalk": "2.0.1",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.13.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.0",
    "enzyme-to-json": "3.2.2",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "0.11.2",
    "history": "4.6.0",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.6",
    "prettier-standard": "^7.0.3",
    "prompt": "1.0.0",
    "prop-types": "15.5.10",
    "raf": "3.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "16.0.0",
    "redux-immutable-state-invariant": "2.0.0",
    "redux-logger": "^3.0.6",
    "replace": "0.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "standard": "^10.0.3",
    "style-loader": "0.19.0",
    "url-loader": "0.5.9",
    "webpack": "3.1.0",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "presets": [
      "stage-1",
      "react"
    ],
    "env": {
      "development": {
        "presets": [
          "env",
          "react-hmre",
          "stage-1"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "stage-1",
          "react"
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env"
        ]
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "jest",
      "expect",
      "fetch",
      "__DEV__",
      "alert",
      "localStorage"
    ],
    "ignore": [
      "/src/webpack-public-path.js"
    ]
  }
}
